import random
import string

print("*****WELCOME TO THE PASSWORD GENERATOR*****")
length=int(input("Enter you desired password length:-"))
lower=string.ascii_lowercase
upper=string.ascii_uppercase
digit=string.digits
symbols=string.punctuation
combine=lower+upper+digit+symbols
p=random.sample(combine,length)
password="".join(p)
print("CONGRATS WE HAVE GENERATED A PASSWORD FOR YOU:\n",password)
